{
  "name": "aurelia-sample",
  "version": "0.2.0",
  "description": "A starter kit for building a standard navigation-style app with Aurelia and webpack.",
  "main": "dist/main.js",
  "scripts": {
    "clean": "del-cli dist",
    "start": "npm run server",
    "server": "npm run webpack-dev-server -- --inline --progress --profile --watch",
    "test": "ts-node ./node_modules/.bin/tape ./src/**/*.spec.ts | tap-spec",
    "webpack-dev-server": "cross-env TS_NODE_FAST=true ./node_modules/.bin/webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kasperstorgaard/aurelia-sample.git"
  },
  "keywords": [
    "aurelia",
    "sample",
    "webpack"
  ],
  "license": "ISC",
  "author": "Kasper Storgaard <kasper.storgaard@gmail.com>",
  "homepage": "https://github.com/kasperstorgaard/aurelia-sample#readme",
  "aurelia": {
    "build": {
      "resources": []
    }
  },
  "dependencies": {
    "aurelia-bootstrapper-webpack": "^1.0.0",
    "aurelia-event-aggregator": "^1.0.0",
    "aurelia-fetch-client": "^1.0.1",
    "aurelia-framework": "^1.0.7",
    "aurelia-history-browser": "^1.0.0",
    "aurelia-loader-webpack": "^1.0.3",
    "aurelia-logging-console": "^1.0.0",
    "aurelia-pal-browser": "^1.0.0",
    "aurelia-polyfills": "^1.1.1",
    "aurelia-route-recognizer": "^1.1.0",
    "aurelia-router": "^1.0.7",
    "aurelia-templating-binding": "^1.0.0",
    "aurelia-templating-resources": "^1.1.1",
    "aurelia-templating-router": "^1.0.0",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^6.0.46",
    "@types/tape": "^4.2.28",
    "@types/webpack": "^1.12.35",
    "@types/whatwg-fetch": "^0.0.32",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "del-cli": "^0.2.0",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "tap-dot": "^1.0.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "testdouble": "^1.9.0",
    "ts-node": "^1.7.0",
    "tslint": "^3.15.1",
    "typescript": "^2.1",
    "wait-on": "^1.5.3",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": ">=2.1.0-beta.0 || ^2.1.0"
  }
}
