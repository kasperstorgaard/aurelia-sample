{
  "name": "aurelia-sample",
  "version": "0.1.1",
  "description": "A starter kit for building a standard navigation-style app with Aurelia and webpack.",
  "main": "dist/main.js",
  "scripts": {
    "clean": "del-cli dist",
    "start": "npm run server",
    "server": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --progress --profile --watch",
